{
	"parser": "babel-eslint",
	"settings": {
		"ecmascript": 6,
		"jsx": true
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"ecmaFeatures": {
		"modules": true,
		"module": true
	},
	"rules": {
		"quotes": [2, "single"],
		"eol-last": [0],
		"no-mixed-requires": [0],
		"no-underscore-dangle": [0],
		"no-unused-vars": 1,
		"strict": false
	}
}